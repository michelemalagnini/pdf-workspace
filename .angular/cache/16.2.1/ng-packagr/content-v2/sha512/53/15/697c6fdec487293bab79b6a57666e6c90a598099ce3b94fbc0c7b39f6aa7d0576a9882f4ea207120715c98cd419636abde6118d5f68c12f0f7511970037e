set();\n            this.isStyleEditorOpen = true;\n        });\n    }\n    export() {\n        return this.ngxPdfEditorService.export(this.element);\n    }\n    deleteElement() {\n        this.ngxPdfEditorService.deleteElement(this.element);\n    }\n    changeInputName(event) {\n        const name = event.target.value;\n        this.textfield.nativeElement.name = name;\n        this.element.value = name;\n    }\n    changeRequired(event) {\n        if (this.element.props) {\n            this.element.props.required = event.target.checked;\n        }\n        this.textfield.nativeElement.focus();\n    }\n    static ɵfac = i0.ɵɵngDeclareFactory({ minVersion: \"12.0.0\", version: \"16.2.3\", ngImport: i0, type: PDFFormTextfieldElement, deps: [{ token: NgxPdfEditorService }, { token: i2$1.DragDrop }], target: i0.ɵɵFactoryTarget.Component });\n    static ɵcmp = i0.ɵɵngDeclareComponent({ minVersion: \"14.0.0\", version: \"16.2.3\", type: PDFFormTextfieldElement, selector: \"lib-form-textfield\", inputs: { element: \"element\" }, host: { properties: { \"style.left\": \"this.left\", \"style.top\": \"this.top\" }, classAttribute: \"pdfElement\" }, viewQueries: [{ propertyName: \"textfield\", first: true, predicate: [\"textfield\"], descendants: true, static: true }], ngImport: i0, template: \"<input\\n  type=\\\"text\\\"\\n  #textfield\\n  #textOverlayOrigin=\\\"cdkOverlayOrigin\\\"\\n  tabindex=\\\"0\\\"\\n  name=\\\"input\\\"\\n  class=\\\"pdf-editor-z-10 pdf-editor-outline-none pdf-editor-p-2 pdf-editor-rounded-md pdf-editor-border pdf-editor-border-solid pdf-editor-border-black focus:pdf-editor-border-gray-600 focus:pdf-editor-border-opacity-100 pdf-editor-cursor-move\\\"\\n  (click)=\\\"onClick()\\\"\\n  (dblclick)=\\\"onDoubleClick()\\\"\\n  (blur)=\\\"onBlur($event)\\\"\\n  cdkOverlayOrigin\\n  [placeholder]=\\\"textfield.name\\\"\\n  readonly\\n/>\\n\\n<ng-template\\n  cdkConnectedOverlay\\n  [cdkConnectedOverlayOrigin]=\\\"textOverlayOrigin\\\"\\n  [cdkConnectedOver
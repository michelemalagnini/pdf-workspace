\n            console.log('EXEC. CLICK EVENT:', event);\n            const target = event.target;\n            if (target.tagName === 'CANVAS') {\n                this.editor.selectedTool?.callback(event);\n            }\n            else {\n                this.pdfEditorService.unselectTool();\n            }\n        };\n        console.log('PAGE RENDERED', pageRendered);\n    }\n    pagesLoaded(event) {\n        this.totalPagesNumber = event.pagesCount;\n        this.pdfRenderCompleted = true;\n    }\n    getTotalPagesNumberArray() {\n        return Array(this.totalPagesNumber)\n            .fill(0)\n            .map((x, i) => i + 1);\n    }\n    render() {\n        this.pdfEditorService.render().then((renderResult) => {\n            console.log('RENDERED COMPLETED');\n            console.log(renderResult);\n            this.dialogRef.close(renderResult);\n        });\n    }\n    changeName(name) {\n        this.editor.filename = name;\n    }\n    makeItAbsolute(index) {\n        console.log('PAGE ABS:', index);\n        const pdfPreview = document.getElementById(`pdf-preview-${index}`);\n        if (pdfPreview) {\n            pdfPreview.firstElementChild?.classList.add('relative');\n        }\n    }\n    static ɵfac = i0.ɵɵngDeclareFactory({ minVersion: \"12.0.0\", version: \"16.2.3\", ngImport: i0, type: NgxPdfEditorComponent, deps: [{ token: MAT_DIALOG_DATA }, { token: i0.ComponentFactoryResolver }, { token: i0.ApplicationRef }, { token: i0.Injector }, { token: NgxPdfEditorService }, { token: i2.MatDialogRef }], target: i0.ɵɵFactoryTarget.Component });\n    static ɵcmp = i0.ɵɵngDeclareComponent({ minVersion: \"14.0.0\", version: \"16.2.3\", type: NgxPdfEditorComponent, selector: \"ngx-pdf-editor\", viewQueries: [{ propertyName: \"pdfViewerHost\", first: true, predicate: [\"pdfViewerHost\"], descendants: true }], ngImport: i0, template: \"<div\\r\\n  class=\\\"pdf-editor-flex pdf-editor-w-full pdf-editor-h-full pdf-editor-p-0 pdf-editor-bg-white\\\"\\r\\n>\\r\\n
eDrag() {\n        this.dragRef = this.dragDrop.createDrag(this.element.componentRef.location);\n        this.dragRef.withBoundaryElement(this.element.parent);\n        this.dragRef.started.subscribe(() => {\n            this.isStyleEditorOpen = false;\n        });\n        this.dragRef.ended.subscribe((_dragRef) => {\n            this.ngxPdfEditorService.updatePosition(this.element);\n            _dragRef.source.reset();\n            this.isStyleEditorOpen = true;\n        });\n    }\n    placeCaretAtEnd() {\n        const selection = window.getSelection();\n        const range = document.createRange();\n        selection?.removeAllRanges();\n        range.selectNodeContents(this.text.nativeElement);\n        range.collapse(false);\n        selection?.addRange(range);\n        this.text.nativeElement.focus();\n    }\n    export() {\n        return this.ngxPdfEditorService.export(this.element);\n    }\n    // setFontBold() {\n    //   this.fontWeight === 'normal' ? this.fontWeight = 'bold' : this.fontWeight = 'normal';\n    // }\n    setFontSize(n) {\n        this.fontSize = `${Number.parseInt(this.fontSize.replace('px', '')) + n}px`;\n        this.element.fontSize = this.fontSize;\n    }\n    changeTextColor(event) {\n        const target = event.target;\n        this.textColor = target.value;\n        this.element.textColor = this.textColor;\n    }\n    deleteElement() {\n        this.ngxPdfEditorService.deleteElement(this.element);\n    }\n    static ɵfac = i0.ɵɵngDeclareFactory({ minVersion: \"12.0.0\", version: \"16.2.3\", ngImport: i0, type: PDFTextElement, deps: [{ token: NgxPdfEditorService }, { token: i2$1.DragDrop }], target: i0.ɵɵFactoryTarget.Component });\n    static ɵcmp = i0.ɵɵngDeclareComponent({ minVersion: \"14.0.0\", version: \"16.2.3\", type: PDFTextElement, selector: \"pdf-text-element\", inputs: { element: \"element\" }, host: { properties: { \"style.left\": \"this.left\", \"style.top\": \"this.top\", \"style.fontSize\": \"this.fontSize\", \"st
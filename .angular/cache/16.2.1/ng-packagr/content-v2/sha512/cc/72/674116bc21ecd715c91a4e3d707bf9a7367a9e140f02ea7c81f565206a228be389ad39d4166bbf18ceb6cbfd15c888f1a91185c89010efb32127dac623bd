}\n    deleteElement() {\n        this.ngxPdfEditorService.deleteElement(this.element);\n    }\n    setImage(ev) {\n        const element = ev.target;\n        const image = element.files ? element.files[0] : null;\n        if (image) {\n            this.ngxPdfEditorService.toBase64(image).then(base64Image => {\n                this.currentImage = base64Image;\n                this.element.value = this.currentImage;\n                this.width = '50%';\n                this.element.width = `${this.image.nativeElement.width}px`;\n                this.element.height = `${this.image.nativeElement.height}px`;\n                this.ngxPdfEditorService.updatePosition(this.element);\n                this.image.nativeElement.click();\n            });\n        }\n    }\n    updateDimension(event) {\n        const target = event.target;\n        this.width = `${target.value}%`;\n        this.element.width = `${this.image.nativeElement.width}px`;\n        this.element.height = `${this.image.nativeElement.height}px`;\n        this.ngxPdfEditorService.updatePosition(this.element);\n        this.image.nativeElement.focus();\n    }\n    static ɵfac = i0.ɵɵngDeclareFactory({ minVersion: \"12.0.0\", version: \"16.2.3\", ngImport: i0, type: PDFImageElement, deps: [{ token: i1.NgxPdfEditorService }, { token: i2.DragDrop }], target: i0.ɵɵFactoryTarget.Component });\n    static ɵcmp = i0.ɵɵngDeclareComponent({ minVersion: \"14.0.0\", version: \"16.2.3\", type: PDFImageElement, selector: \"image-element\", host: { properties: { \"style.top\": \"this.top\", \"style.left\": \"this.left\", \"style.width\": \"this.width\" }, classAttribute: \"pdfElement\" }, viewQueries: [{ propertyName: \"imageInput\", first: true, predicate: [\"imageInput\"], descendants: true, static: true }, { propertyName: \"image\", first: true, predicate: [\"image\"], descendants: true, static: true }], ngImport: i0, template: \"<div\\n  #image\\n  #imageOverlayOrigin=\\\"cdkOverlayOrigin\\\"\\n  cdkOverlayOrigin\\n  